{% include "parts/_template_start_.njk" %}{# ante process #}

{%- set _template_s %}
<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd"
  xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:ca="http://11tyframe.netlify.com/schemas/ca_schema">
{%- for _post_o in collections.all %}
  <url>
    <loc>{{ U_o.url_s + _post_o.data.permalink }}</loc>
    <lastmod>{{ _post_o.data.date }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
{%- endfor %}
</urlset>
{% endset -%}

{% include "parts/_template_end_.njk" %}{# post process #}
